{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Attachment",
  "type": "object",
  "required": [
    "filename",
    "content"
  ],
  "properties": {
    "filename": {
      "type": "string",
      "minLength": 1
    },
    "content": {
      "type": "string",
      "minLength": 1
    },
    "isSummarized": {
      "type": "boolean"
    },
    "originalSize": {
      "type": "string"
    },
    "summarizationLevel": {
      "type": "string"
    },
    "contentPreserved": {
      "type": "string"
    },
    "processingLimitation": {
      "type": "string"
    }
  }
}